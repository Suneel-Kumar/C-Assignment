@model IEnumerable<ConceptAcademy01.Models.Student>
@{
    ViewBag.Title = "Suneel1";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Suneel1</h2>

@if (Model != null)
{
    <table>
        <tr>
            <td>ID : </td>
            <td>Name : </td>
            <td>Age : </td>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ID</td>
                <td>@item.Name</td>
                <td>@item.Age</td>
            </tr>
        }
    </table>
}


